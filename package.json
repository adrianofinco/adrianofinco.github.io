{
  "name": "adrianofinco.github.io",
  "version": "3.0.0",
  "description": "My portfolio website",
  "private": true,
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "cssnano": "^4.1.10",
    "html-minifier": "^4.0.0",
    "postcss-cli": "^8.1.0",
    "tailwindcss": "^1.8.12"
  },
  "scripts": {
    "css": "postcss src/css/tailwind.css -o _site/app.css",
    "css:prod": "postcss src/css/tailwind.css -o _site/app.css -- BUILD_ENV=production",
    "clean": "del /s /q _site\\*",
    "11ty": "eleventy",
    "11ty:prod": "npm run clean && eleventy -- BUILD_ENV=production",
    "build": "npm run 11ty:prod && npm run css:prod"
  },
